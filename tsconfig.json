{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "sourceMap": true,
    "module": "commonjs",
    "target": "es2017",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@lib/*": [
        "./lib/*"
      ],
      "@helpers/*": [
        "./helpers/*"
      ],
      "@dbTransactions/*": [
        "./dbTransactions/*"
      ],
      "@interfaces/*": [
        "./interfaces/*"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
}