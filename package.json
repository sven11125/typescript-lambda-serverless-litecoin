{
  "name": "kuky-litecoin",
  "version": "1.0.0",
  "description": "Web Service of Kuky litecoin",
  "main": "index.js",
  "scripts": {
    "start:user": "cd services/apis && node --max-old-space-size=4096 ../../node_modules/serverless/bin/serverless.js offline start --webpack-use-polling=500 --debugOffline",
    "deploy:user:dev": "cd services/apis && sls deploy -v --stage dev",
    "remove:user:dev": "cd services/apis && sls remove -v --stage dev",
    "generate:migration:dev": "ts-node node_modules/typeorm/cli.js migration:generate -n base_structure -c dev",
    "migrate:dev": "ts-node node_modules/typeorm/cli.js migration:run -c dev",
    "migrate:revert:dev": "ts-node node_modules/typeorm/cli.js migration:revert -c dev",
    "test": "node_modules/.bin/jest",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "serverless",
    "lambda",
    "kuky-litecoin",
    "back service"
  ],
  "author": "Mykhailo",
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 105
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.61",
    "@types/hapi__joi": "^17.1.4",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.6.0",
    "@types/pg": "^7.14.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "await-to-js": "^2.1.1",
    "aws-sdk": "^2.779.0",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "bitcoin-core": "^3.0.0",
    "cloudinary": "^1.22.0",
    "concurrently": "^5.3.0",
    "date-fns": "^2.16.1",
    "dynamoose": "^2.3.0",
    "eslint": "^7.7.0",
    "fs": "0.0.1-security",
    "increase-memory-limit": "^1.0.7",
    "jest": "^26.4.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.9.0",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.27",
    "mysql2": "^2.2.5",
    "pg": "^8.4.2",
    "prettier": "^2.0.5",
    "serverless": "^1.79.0",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-offline": "^6.5.0",
    "serverless-plugin-resource-tagging": "^1.0.11",
    "serverless-webpack": "^5.3.3",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.19",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typeorm": "^0.2.25",
    "typeorm-naming-strategies": "^1.1.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  }
}
